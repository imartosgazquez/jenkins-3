pipeline{
 agent any
stages{
 stage("Clear WorkSpace"){
 steps{
 script{
 sh "rm -rf ./*"
 }

 }
 }
 stage("Git Clone"){
 steps{
 withCredentials([usernamePassword(credentialsId:'github_credencials', usernameVariable:'CUSTOM_USER', passwordVariable:'CUSTOM_PASSWORD')]){
 script{
 sh "git clone https://$CUSTOM_USER:CUSTOM_PASSWORD@github.com/imartosgazquez/desenrotllament-de-Interficies"
 }
 }
 }
 }
 stage("Trigger Job"){
 steps{
 script{
 build job: 'Pipeline-2'
 }
 }
 }

}
}